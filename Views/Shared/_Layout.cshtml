<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title  iBAS</title>
    @Styles.Render("~/Content/css")
    @*@Scripts.Render("~/bundles/modernizr")*@
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />
    @*<script src="~/Scripts/jquery-1.12.4.min.js"></script>*@
    <script src="~/Scripts/jquery-3.6.0.js"></script>
    <link href="~/Content/font-awsome-min.css" rel="stylesheet" />
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jqueryui.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.datetimepicker.full.min.js")"></script>
    <script src="@Url.Content("~/Scripts/font.js")"></script>
    <script src="@Url.Content("~/Scripts/pdfmake.min.js")"></script>
    <script src="@Url.Content("~/Scripts/html2canvas.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.table2excel.min.js")"></script>
    <script src="@Url.Content("~/Scripts/JsPDF.js")"></script>
    <script src="~/Scripts/Data.js"></script>

    <style>
        /* Remove the navbar's default margin-bottom and rounded borders */
        .navbar {
            margin-bottom: 0;
            border-radius: 0;
            background-color: rgb(70, 80, 82);
            min-height: 45px;
             /*min-height: 80px;*/
        }

        .navbar-inverse {
            /*background-color: rgb(70, 80, 82);*/
            background-color: #063146;
            border-color: rgb(70, 80, 82);
            color: white !important;
        }

        .navbar-brand {
            color: white;
        }

        /* Add a gray background color and some padding to the footer */
        footer {
            background-color: rgb(70, 80, 82);
            padding: 0px;
            position: fixed;
            left: 0;
            bottom: 0;
            width: 100%;
            color: white;
            font-size: 12px;
        }

        @@media print {
            .tt {
                /*margin-left: auto  !important;
                  margin-right: auto  !important;*/
                padding-top: 20px !important;
            }

            .bb {
                margin-right: 10px !important;
                text-align: left !important;
                background-color: black !important;
                color: white !important;
            }
        }
    </style>
</head>
@*<body style="background-color:#f2f2f2">*@
<body style="background-color:white">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container-fluid" style="background-color:063146">
            @*#063146*@
            @*#659EC7*@
            <div class="navbar-header">
                <a class="navbar-brand" href="#"><b>iBAS</b></a>
                <input type="button" id="btnBack" value="Home" class="btn" style="background-color:#063146;color:white;font-size: x-small;" onclick="goBack();" />
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav"></ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>

    <div class="container-fluid bg-3">

        <div class="col-sm-12" style="padding-top:10px">
            <div class="row content">
                @RenderBody()

            </div>
        </div>
    </div>
    <footer class="container-fluid text-center">
        <p>&copy; @DateTime.Now.Year - Kores (India) Ltd</p>
    </footer>
    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)*@
</body>
</html>
