
<div id="ajaxorg" style="padding-left:10%">
    <label class="w3-label w3-text-black" style="font-weight:600"> Select Customer:</label>
    <input type="text" value="@ViewBag.CustCount" id="CustCount" hidden/>
    <br />
    @Html.DropDownList("CustAccessPartialView", null, new { @multiple = true, @name = "CustAccessPartialView[]" })
</div>


<script src="~/Scripts/jquery.multiselect.js"></script>

<script type="text/javascript">
    $('#CustAccessPartialView').multiselect({
        columns: 2,
        placeholder: 'Select Customer'
    });
</script>
